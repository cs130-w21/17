{
  "name": "mern_typescript",
  "version": "1.0.0",
  "description": "EasyMeet Server Side Dependencies",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "npm run server",
    "server": "nodemon server.js --exec babel-node --presets babel-preset-env",
    "client": "npm start --prefix client",
    "test": "jest",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json README.md",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "EasyMeet Team",
  "license": "MIT",
  "dependencies": {
    "@jest/globals": "^26.6.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^3.6.0",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.1",
    "googleapis": "^67.1.0",
    "jsdoc-route-plugin": "^0.1.0",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.1.6",
    "node-core-lib": "^0.11.11",
    "nodemailer": "^6.4.18",
    "react-google-login": "^5.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.3",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "jsdoc": "^3.6.6",
    "mocha": "^8.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.17.5",
    "supertest": "^6.1.3"
  }
}
